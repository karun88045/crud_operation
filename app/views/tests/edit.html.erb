<h4 class="py-2 text-center">Update To Test</h4>
<div class="container">
	<div class="row">
		<div class="col-md-6 col-lg-6 col-sm-12 mx-auto card">
			<%= form_with model: @test, :method => 'patch' do |u| %>
			<div class="mb-3">
				<%= u.label :Name , class: 'mb-2' %>
				<%= u.text_field :name , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= u.label :Email , class: 'mb-2' %>
				<%= u.email_field :email , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= u.label :Password , class: 'mb-2' %>
				<%= u.password_field :password , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= u.label :Mobile , class: 'mb-2' %>
				<%= u.number_field :mobile , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= u.submit class: 'btn btn-primary' %>
			</div>
		</div>
	</div>
	<% end %>
</div>