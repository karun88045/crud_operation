<h4 class="py-2 text-center">Add To Test</h4>
<div class="container">
	<div class="row">
		<div class="col-md-6 col-lg-6 col-sm-12 mx-auto card">
			<%= form_with model: @test do |form| %>
			<div class="mb-3">
				<%= form.label :Name , class: 'mb-2' %>
				<%= form.text_field :name , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= form.label :Email , class: 'mb-2' %>
				<%= form.email_field :email , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= form.label :Password , class: 'mb-2' %>
				<%= form.password_field :password , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= form.label :Mobile , class: 'mb-2' %>
				<%= form.number_field :mobile , class: 'form-control' %>
			</div>
			<div class="mb-3">
				<%= form.submit class: 'btn btn-primary' %>
			</div>
		</div>
	</div>
	<% end %>
</div>